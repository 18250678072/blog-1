{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/op/linux-monitor", "destination": "/op/linux-monitor.html", "permanent": true }
  ]
}
